<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="417.0" prefWidth="584.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.grid_pane.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label alignment="TOP_LEFT" text="Formular pro registraci noveho uzivatele do systemu" />
      <GridPane prefHeight="90.0" prefWidth="459.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="104.0" minWidth="10.0" prefWidth="102.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="163.0" minWidth="10.0" prefWidth="163.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="104.0" minWidth="10.0" prefWidth="98.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="237.0" minWidth="10.0" prefWidth="156.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField prefHeight="25.0" prefWidth="131.0" GridPane.columnIndex="3" />
            <TextField GridPane.columnIndex="1" />
            <Label text="Prijmeni:" GridPane.columnIndex="2" />
            <Label text="Jmeno:" />
            <Label text="login:" GridPane.rowIndex="2" />
            <DatePicker GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Datum Narozeni:" GridPane.rowIndex="1" />
            <Label text="Oblibena barva:" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Label text="Heslo:" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField GridPane.columnIndex="3" GridPane.rowIndex="2" />
            <ColorPicker GridPane.columnIndex="3" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <HBox prefHeight="13.0" prefWidth="544.0">
         <children>
            <Label text="Jazyky ktere ovladam:" />
            <CheckBox mnemonicParsing="false" text="Anglictina" />
            <CheckBox mnemonicParsing="false" text="Cestina" />
            <CheckBox mnemonicParsing="false" text="Nemcina" />
            <CheckBox mnemonicParsing="false" text="Spanelstina" />
         </children>
      </HBox>
      <HBox prefHeight="15.0" prefWidth="544.0">
         <children>
            <Label text="Souhlasim s podminkami:" />
            <RadioButton mnemonicParsing="false" text="Ano">
               <toggleGroup>
                  <ToggleGroup fx:id="souhlas" />
               </toggleGroup></RadioButton>
            <RadioButton mnemonicParsing="false" text="Ne" toggleGroup="$souhlas" />
         </children>
      </HBox>
      <TextArea prefHeight="143.0" prefWidth="544.0" text="Prostor pro poznamky" />
      <Button mnemonicParsing="false" text="Login" />
   </children>
</VBox>
